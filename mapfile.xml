<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map bgcolor="steelblue" srs="+proj=latlong +datum=WGS84">
    <!-- begin styles -->
    <Style name="world_style">
        <Rule>
            <PolygonSymbolizer>
                <CssParameter name="fill">#f2eff9</CssParameter>
            </PolygonSymbolizer>
            <LineSymbolizer>
                <CssParameter name="stroke">rgb(50%,50%,50%)</CssParameter>
                <CssParameter name="stroke-width">0.1</CssParameter>
            </LineSymbolizer>
        </Rule>
    </Style>
    <!-- begin layers -->
    <Layer name="world" srs="+proj=latlong +datum=WGS84">
        <StyleName>world_style</StyleName>
        <Datasource>
            <Parameter name="type">shape</Parameter>
            <Parameter name="file">/home/h0st1le/webapps/template/papyrus_mapnik/shapefiles/world_borders.shp</Parameter>
        </Datasource>
    </Layer>
</Map>
